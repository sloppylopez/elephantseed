{
  "name": "slush-jspm-react-seed",
  "description": "Slush for scaffolding, Jspm for packaging, React as templating framework",
  "version": "1.0.0",
  "homepage": "https://github.com/sloppylopez/slush-jspm-react-seed",
  "author": {
    "name": "Sergio Lopez",
    "email": "sloppy@sloppylopez.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sloppylopez/slush-jspm-react-seed.git"
  },
  "bugs": {
    "url": "https://github.com/sloppylopez/slush-jspm-react-seed/issues"
  },
  "jspm": {
    "main": "templates/",
    "dependencies": {
      "bootstrap": "github:twbs/bootstrap@^3.3.6",
      "classnames": "npm:classnames@^2.2.1",
      "css": "github:systemjs/plugin-css@^0.1.20",
      "express": "npm:express@^4.13.3",
      "history": "npm:history@^1.13.1",
      "jquery": "npm:jquery@^2.1.4",
      "react": "npm:react@^0.14.3",
      "react-bootstrap": "npm:react-bootstrap@^0.28.1",
      "react-dom": "npm:react-dom@^0.14.3",
      "react-router": "npm:react-router@^1.0.0",
      "react-router-bootstrap": "npm:react-router-bootstrap@^0.19.3",
      "scss": "github:mobilexag/plugin-sass@^0.1.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "clean-css": "npm:clean-css@^3.4.8",
      "core-js": "npm:core-js@^1.1.4"
    }
  },
  "devDependencies": {
    "jspm": "^0.16.15"
  },
  "scripts": {
    "start": "npm run unbundle && jspm run server",
    "start:dist": "npm run minify && jspm run server",
    "bundle": "jspm bundle app dist/bundle.js --inject",
    "minify": "jspm bundle app dist/bundle.js --inject --minify",
    "unbundle": "jspm unbundle",
    "babel": "babel . --out-dir ES5_transpiled/",
    "cache": "npm run babel && jspm depcache ES5_transpiled/app.js",
    "cache-clear": "echo \"jspm cache-clear works buggy somehow.. AND cache as well.. need more investigation, possible bug in jspm\"",
    "ti": "jspm config registries.npm.timeouts.lookup 240",
    "clean:tree": "jspm i -f",
    "ci": "jspm config registries.npm.timeouts.lookup 2000"
  }
}
